<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	                       http://www.springframework.org/schema/beans/spring-beans.xsd
	                       http://www.springframework.org/schema/context
	                       http://www.springframework.org/schema/context/spring-context.xsd">

<bean id ="student" class= "com.examples.spring.autowire.Student" autowire="byType">

<property name="name" value="Vinod"></property>
<property name="id" value="001"></property>

</bean>  

<bean id ="Hostel" class= "com.examples.spring.autowire.Hostel">

<property name="hostelName" value="YMCA"></property>
<property name="city" value="EKM"></property>

</bean>  

	
	
</beans>
